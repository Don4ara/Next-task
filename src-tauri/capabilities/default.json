{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "updater:default",
    "log:default",

    "updater:default",
    "updater:allow-check",

    "dialog:default",
    "dialog:allow-message"
  ]
}
